const int led = D7;
void setup()
{
    pinMode(led, OUTPUT);
}


void loop()
{
    digitalWrite(led, HIGH);
    
    //S
    digitalWrite(led, HIGH);
    delay(300);
    digitalWrite(led, LOW);
    delay(500);
    digitalWrite(led, HIGH);
    delay(300);
    digitalWrite(led, LOW);
    delay(500);
    digitalWrite(led, HIGH);
    delay(300);
    digitalWrite(led, LOW);
    delay(500);
    
    //O
    digitalWrite(led, HIGH);
    delay(1000);
    digitalWrite(led, LOW);
    delay(500);
    digitalWrite(led, HIGH);
    delay(1000);
    digitalWrite(led, LOW);
    delay(500);
    digitalWrite(led, HIGH);
    delay(1000);
    digitalWrite(led, LOW);
    delay(500);
    
    //M
    digitalWrite(led, HIGH);
    delay(1000);
    digitalWrite(led, LOW);
    delay(500);
    digitalWrite(led, HIGH);
    delay(1000);
    digitalWrite(led, LOW);
    delay(500);
    
    //I
    digitalWrite(led, HIGH);
    delay(300);
    digitalWrite(led, LOW);
    delay(500);
    digitalWrite(led, HIGH);
    delay(300);
    
    //L
    digitalWrite(led, HIGH);
    delay(300);
    digitalWrite(led, LOW);
    delay(500);
    digitalWrite(led, HIGH);
    delay(300);
    digitalWrite(led, HIGH);
    delay(1000);
    digitalWrite(led, LOW);
    delay(500);
    digitalWrite(led, HIGH);
    delay(300);
    digitalWrite(led, LOW);
    delay(500);
    digitalWrite(led, HIGH);
    delay(300);
    digitalWrite(led, HIGH);
    delay(300);
    digitalWrite(led, LOW);
    delay(500);
    digitalWrite(led, HIGH);
    delay(300);
}